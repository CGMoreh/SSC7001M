my.summary <- function(x, na.rm=TRUE){
result <- c(N=length(x[!is.na(x)]),
Mean=round(mean(x, na.rm=na.rm), 3),
SD=round(sd(x, na.rm=na.rm), 3),
Min=min(x, na.rm=na.rm),
Max=max(x, na.rm=na.rm))
}
